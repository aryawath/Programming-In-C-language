#include <stdio.h>
#include <math.h>

// Function to check if a number is prime
int is_prime(int n) {
	
	int flag = 1;

	for (int i = 2; n > i; i++){
		if (n % i == 0){
			flag = 0;
			break;
		} else {
			flag = 1;
		}
}

	return flag;

	
}

// Function to check if a number is an Armstrong number
int is_armstrong(int n) {
	
	int original = n, calc = 0, num = n;
	int digits = 0;

	while (num > 0) {
		digits++;
		num /= 10;
}
	num = n;
	while (num > 0){
		int digit = num % 10;
		calc += pow(digit, digits);
		num /= 10;
	}

	return (calc == original) ? 1 : 0;
}

int main() {
	int n;
	
	// Input from the user
	scanf("%d", &n);
	
	// Check if the number is prime
	if (is_prime(n))
		printf("%d is prime\n", n);
	else
		printf("%d is not prime\n", n);
	
	// Check if the number is an Armstrong number
	if (is_armstrong(n))
		printf("%d is Armstrong\n", n);
	else
		printf("%d is not Armstrong\n", n);
	
	return 0;
}
