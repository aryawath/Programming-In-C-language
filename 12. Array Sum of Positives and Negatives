#include <stdio.h>

int main() {
	
    int n, pos = 0, neg = 0;
	
 scanf("%d", &n);

	int arr[n];

  for(int i = 0; i < n; i++) {
		  scanf("%d", &arr[i]);

	  if (arr[i] > 0) {
		pos += arr[i];
	  } else if (arr[i] < 0){
	neg += arr[i];
	  }

  }
	printf("%d\n%d\n", pos, neg);

	return 0;
}



    //input size
    //printf("enter the value")
