// Type Content here...
#include <stdio.h>

int main() {
	    int num1, num2;

	    // Input two integers

	    scanf("%d", &num1);
	    scanf("%d", &num2);

	printf("%d\n", num1 + num2);
	printf("%d\n", num1 - num2);
	printf("%d\n", num1 * num2);
	printf("%d\n", num1 / num2);
	printf("%d\n", num1 % num2);

	return 0;
}
